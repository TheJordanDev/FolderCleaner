{
    "theme": "dark_amber.xml",
    "filters": [
        {
            "name": "Images",
            "expressions": [
                "*.jpg",
                "*.png",
                "*.jpeg",
                "*.gif",
                "*.bmp"
            ],
            "folder": "Images"
        },
        {
            "name": "Videos",
            "expressions": [
                "*.mp4",
                "*.mkv",
                "*.avi",
                "*.mov",
                "*.flv"
            ],
            "folder": "Videos"
        },
        {
            "name": "Documents",
            "expressions": [
                "*.doc",
                "*.docx",
                "*.pdf",
                "*.txt",
                "*.ppt",
                "*.pptx",
                "*.xls",
                "*.xlsx"
            ],
            "folder": "Documents"
        },
        {
            "name": "Music",
            "expressions": [
                "*.mp3",
                "*.wav",
                "*.ogg",
                "*.flac",
                "*.wma"
            ],
            "folder": "Music"
        },
        {
            "name": "Applications",
            "expressions": [
                "*.exe",
                "*.msi",
                "*.dmg",
                "*.deb",
                "*.rpm"
            ],
            "folder": "Applications"
        },
        {
            "name": "Archives",
            "expressions": [
                "*.zip",
                "*.rar",
                "*.7z",
                "*.tar"
            ],
            "folder": "Archives"
        },
        {
            "name": "Scripts",
            "expressions": [
                "*.py",
                "*.js",
                "*.sh",
                "*.bat",
                "*.cmd"
            ],
            "folder": "Scripts"
        },
        {
            "name": "Others",
            "expressions": [
                "*"
            ],
            "folder": "Others"
        }
    ],
    "targets": [
        {
            "name": "Téléchargements",
            "path": "C:/Users/Jordan/Desktop"
        }
    ]
}